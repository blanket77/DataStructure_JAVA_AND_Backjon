package Stack;

import java.util.EmptyStackException;

class Node{
	int data;
	Node next = null;
	
	Node(){};
	Node(int d){
		this.data = d;
	}
}

class Stack{
	Node top = null;
	
	public void push(int d)
	{
		 Node n = new Node(d);
		 n.next = top;
		 top = n;
	}
	
	public int pop() {
		if (top == null) {
			throw new EmptyStackException();
		}
		int r = top.data;
		top = top.next;
		return r;
	}
	
	public void peek() {
		if (top == null) {
			throw new EmptyStackException();
		}
		Node sel = top;
		while(sel.next != null)
			sel = sel.next;
		System.out.println(sel.data);
	}
}

public class StackPro {
	public static void main(String args[])
	{
		Stack s = new Stack();
		s.push(3);
		s.push(4);
		s.push(2);
		s.push(1);
		s.push(5);
		s.peek();
		System.out.println(s.pop());
		System.out.println(s.pop());
		System.out.println(s.pop());
		System.out.println(s.pop());
		System.out.println(s.pop());
	}

}
