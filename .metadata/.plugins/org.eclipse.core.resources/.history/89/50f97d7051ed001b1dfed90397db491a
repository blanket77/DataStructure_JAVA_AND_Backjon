package BinaryTreeTraversal;

class Node{
	Node left;
	Node right;
	int data;
	Node next = null;
	Node(){};
	Node(int d){
		this.data = d ;
	}
}

class Tree{
	Node root;
	
	public void setRoot(Node node) {
		this.root = node;
	}
	
	public Node getRoot() {
		return root;
	}
	
	public Node makeNode(int data, Node left, Node right) {
		Node node = new Node();
		node.left = left;
		node.right = right;
		return node;
	}
	
	public void Inorder() {
		
	}
}

