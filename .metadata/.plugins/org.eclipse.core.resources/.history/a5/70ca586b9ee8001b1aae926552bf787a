class Node{
	int data;
	Node next = null;
	Node(){}
	

	
	Node(int d){
		this.data = d;
	}
	
	void append(int d)
	{
		Node end = new Node(d);
		Node n = this;
		while(n.next != null)
			n = n.next;
		n.next = end;
	}
	
	void delete(int d){
		Node n = this;
		while(n.next !=null){
			if(n.next.data ==d){
				n.next = n.next.next;
				break;
			}
			n = n.next;
		}
	}
	
	void retrieve() {
		Node n = this;
		while(n.next != null) {
			System.out.println(n.data+ " => ");
			n = n.next;
			}
		n
	
	}
	}

public class LinkedList {
	public static void main(String[] arg) {
		
		
	}
}
